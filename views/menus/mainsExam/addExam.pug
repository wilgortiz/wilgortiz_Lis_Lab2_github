div(class="mt-3 px-2 d-flex flex-column w-100 h-100 rounded") 
    div(class="position-absolute top-50 start-50 translate-middle d-flex invisible" id="invisible" style="z-index:1")
        div.border.rounded(id="contenedor-flotante")           
    p(class="letraChida h4") Add Exam
    span(class="letraChida h6 text-secondary") Enter Exam information for registration.
    div(class="pt-5 w-100")
        .row.border.pb-5.rounded.bg-white.shadow-sm(id="contenido-exam")
            legend(class="letraChida h5 p-3 bg-color rounded text-white") Exam:
            div(class="my-4 col position-relative d-flex flex-column justify-content-start w-50 h6")    
                label(for="nameExam") Name:
                input(class="inputses2 h6 m-2 p-3" id="nameExam" name="nameExam" type="text")
            div(class="col position-relative d-flex flex-column justify-content-start w-50 h6")    
                textarea(class="h6 m-2 p-3" placeholder="Detail.." id="detailExam" name="detailExam" , cols="30", rows="3")
            .w-100.d-flex.justify-content-center.mt-2 
                button(class="btn btn-primary rounded text-white w-25" id="btnAddExam") Add 
                    i(class="fa-solid fa-plus px-1 text-white") 
        .row.border.my-4.rounded.bg-white.shadow-sm.invisible(id="contenido-examSubGroup" )
                .d-flex.flex-row.bg-color.rounded.text-white.w-100
                    legend(class="letraChida h5 p-3") Exam Subgroup:
                    .d-flex.justify-content-center.p-3
                        button(class="btn btn-primary rounded text-white" id="btnAddSubGroup")
                            i(class="fa-solid fa-plus px-1 text-white") 
                div(class="table-responsive mt-4")
                    table(class="table table-bordered table-hover h6 mt-3")
                        thead(class="")
                            tr(class="")
                                th(scope="col") ID
                                th(scope="col") Name
                                th(scope="col") Detail
                                th(scope="col") Determinat
                                th(scope="col") Delete
                        tbody.invisible
                                    tr 
                                        th(scope="row") 
                                        td 
                                        td 
                                        td 
                                            div(class="w-100 text-center")
                                                a(href="" id="" class="viewDeterminants")
                                                    i(class="fa-solid fa-circle-plus px-1 text-success") 
                                        td 
                                            div(class="w-100 text-center")
                                                a(href="" id="" class="btnDeleteEsg")
                                                    i(class="fa-solid fa-trash px-1 text-danger")
        .row.border.my-4.rounded.bg-white.shadow-sm.invisible(id="contenido-determinant")
                .d-flex.flex-row.bg-color.rounded.text-white.w-100
                    legend(class="letraChida h5 p-3") Determinant
                    .d-flex.justify-content-center.p-3
                        button(class="btn btn-primary rounded text-white" id="btnAddDeterminant")
                            i(class="fa-solid fa-plus px-1 text-white")
                div(class="table-responsive mt-4")
                    table(class="table table-bordered table-hover h6 mt-3")
                        thead(class="")
                            tr(class="")
                                th(scope="col") ID
                                th(scope="col") Name
                                th(scope="col") Abreviation
                                th(scope="col") Measurement
                                th(scope="col") Reference Value 
                                th(scope="col") Delete 
                        tbody
                                    tr 
                                        th(scope="row") 
                                        td 
                                        td 
                                        td 
                                        td 
                                            div(class="w-100 text-center")
                                                a(href="" id="" class="")
                                                    i(class="fa-solid fa-circle-plus px-1 text-success") 
                                        td 
                                            div(class="w-100 text-center")
                                                a(href="" id="" class="btnDeleteDeterminant")
                                                    i(class="fa-solid fa-trash px-1 text-danger")

